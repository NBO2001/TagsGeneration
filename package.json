{
    "name": "back-server",
    "version": "1.0.0",
    "scripts": {
        "client": "cd front && npm start",
        "serv": "cd front && serve -s build",
        "server": "nodemon app.js",
        "inst": "npm install && cd front && npm install && npm rum build",
        "update": "git pull && npm rum inst",
        "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
        "proc": "concurrently --kill-others-on-fail \"npm run server\" \"npm run serv\""
    },
    "dependencies": {
        "@material-ui/core": "^4.12.3",
        "@material-ui/icons": "^4.11.2",
        "cd": "^0.3.3",
        "concurrently": "^3.6.1",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "express": "^4.16.2",
        "formidable": "^1.2.2",
        "front": "^0.0.1",
        "nodemon": "^2.0.20",
        "sequelize": "^6.6.5",
        "sqlite3": "^5.0.2",
        "styled-reset": "^4.3.4"
    },
    "devDependencies": {
        "concurrently": "^3.6.1"
    }
}
